<odoo>
    <data>
        <record id="view_token_form" model="ir.ui.view">
            <field name="name">academico token form</field>
            <field name="model">academico.token</field>
            <field name="arch" type="xml">
                <form string="token">
                    <group>
                       
                        <field name="name"/>
                <field name="token"/>
                <field name="user_id"/>
                    </group>
                </form>
            </field>
        </record>
      
      <record id="view_token_tree" model="ir.ui.view">
        <field name="name">academico token list</field>
        <field name="model">academico.token</field>
        <field name="arch" type="xml">
            <tree string="token">
            
                <field name="name"/>
                <field name="token"/>
                <field name="user_id"/>

                
            </tree>
        </field>
      </record>

      <record model="ir.actions.act_window" id="academico.action_token_window">
        <field name="name">Registro de token</field>
        <field name="res_model">academico.token</field>
        <field name="view_mode">tree,form</field>
      </record>
      
      <menuitem name="token" id="academico.menu_1_token_list" parent="academico.menu_1"
      action="academico.action_token_window"/>
    </data>
</odoo>